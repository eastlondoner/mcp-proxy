{
  "name": "mcp-proxy",
  "version": "0.1.0",
  "description": "MCP proxy server with static tools for managing dynamic backend MCP servers",
  "type": "module",
  "main": "dist/server.js",
  "exports": {
    ".": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eastlondoner/mcp-proxy.git"
  },
  "author": "eastlondoner",
  "license": "MIT",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "proxy",
    "llm",
    "tools",
    "ai"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "bun run src/server.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "check": "bun run typecheck && bun run lint"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  }
}
